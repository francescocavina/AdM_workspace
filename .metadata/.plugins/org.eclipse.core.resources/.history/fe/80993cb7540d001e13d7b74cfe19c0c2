/* EJERCICIO 1 ---------------------------------------------------------------*/
void c_zeros_tester(void) {

	uint32_t myVec[] = {1, 3, 5, 7, 9, 11};

	zeros(myVec, sizeof(myVec) / sizeof(myVec[0]);
}


