#include <c_func.h>


/* EJERCICIO 1 ---------------------------------------------------------------*/
void c_zeros(uint32_t *vector, uint32_t longitud) {

	for(; longitud > 0; longitud--) {
		vector[longitud-1] = 0;
	}
}
